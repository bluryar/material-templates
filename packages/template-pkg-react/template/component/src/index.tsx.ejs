import * as React from 'react';
import styles from './index.module.css';

interface ComponentProps {
  /** Title for <%= className %>. */
  title: string;
}

export default function <%= className %>(props: ComponentProps) {
  const { title = 'Hello World!' } = props;

  return (
    <div className={styles.<%= className %>}>{title}</div>
  );
}
