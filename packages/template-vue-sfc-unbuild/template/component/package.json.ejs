{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "componentConfig": {
    "name": "<%= name %>",
    "title": "<%= title %>",
    "category": "<%= category %>"
  },

  "type": "module",
  "packageManager": "pnpm@8.15.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": "",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "bump:patch": "npx bumpp patch --all --yes",
    "bump:minor": "npx bumpp minor --all --yes",
    "bump:major": "npx bumpp major --all --yes",
    "prepublishOnly": "pnpm build",
    "screenshots": "tsx ./scripts/screenshot.ts",
    "postinstall": "npx simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.6",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@ice/screenshot": "^0.1.9",
    "@types/node": "^20.11.16",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "bumpp": "^9.4.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "eslint": "^8.57.0",
    "happy-dom": "^14.3.10",
    "lint-staged": "^15.2.2",
    "mkdist": "^1.4.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unbuild": "2.0.0",
    "vite": "^5.0.12",
    "vitest": "^1.4.0",
    "vue": "^3.4.15"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no -- commitlint --edit $1",
    "pre-push": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,mts,cts,md,html,yaml,yml}": [
      "eslint --fix"
    ]
  }
}
